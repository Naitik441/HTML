			document.backlogForm.dob.value = '';
			document.backlogForm.age.value = '';

			return false;

		}

		if (year == (edYear - 16)) {
			if (month > edMonth) {
				alert("The Diff. between Issue Date and DOB must be greater than or equal to 16 Yrs");
				document.getElementById("dob").disabled = false;
				document.getElementById("age").disabled = false;

				document.backlogForm.dob.value = '';
				document.backlogForm.age.value = '';

