				}
			});
}

function addresscopy() {

	if (document.getElementById("copy").checked) {

		document.getElementById("pmState").value = document
				.getElementById("prState").value;
		var options1 = "<option value='select'>Select</option>";
		options1 += "<option value='"
				+ $('#prDistrict').val()
				+ "'>"
				+ $(
						"#prDistrict option[value='" + $('#prDistrict').val()
								+ "']").text() + "</option>";
		$("#pmDistrict").html(options1);
		document.getElementById("pmDistrict").value = $('#prDistrict').val();

		options = "<option value='select'>Select</option>";
		options += "<option value='"
				+ $('#prSubDistrict').val()
				+ "'>"
				+ $(
						"#prSubDistrict option[value='"
								+ $('#prSubDistrict').val() + "']").text()
				+ "</option>";
		$("#pmSubDistrict").html(options);
		document.getElementById("pmSubDistrict").value = $('#prSubDistrict')
				.val();

		var options = "<option value='select'>Select</option>";
		options += "<option value='"
				+ $('#prTown').val()
				+ "'>"
				+ $("#prTown option[value='" + $('#prTown').val() + "']")
						.text() + "</option>";
		$("#pmTown").html(options);
		document.getElementById("pmTown").value = $('#prTown').val();

		$('label[for="pmSD"]').text($('label[for="prSD"]').text());

		if ($('#presVillageOrTown1').is(':checked')) {
			$('#permVillageOrTown1').prop('checked', true);
		} else {
			$('#permVillageOrTown2').prop('checked', true);
		}
		document.getElementById("pmLocation").value = document
				.getElementById("prLocation").value;
		document.getElementById("pmHouseNo").value = document
				.getElementById("prHouseNo").value;
		document.getElementById("pmStreet").value = document
				.getElementById("prStreet").value;

		document.getElementById("pmPinCode").value = document
				.getElementById("prPinCode").value;

		// alert("prState : " + document.getElementById("prState").value);
		document.getElementById("pmState").value = document
				.getElementById("prState").value;

		document.getElementById("pmHouseNo").disabled = true;
		document.getElementById("pmStreet").disabled = true;
		document.getElementById("pmTown").disabled = true;
		document.getElementById("pmPinCode").disabled = true;
		document.getElementById("pmState").disabled = true;
		document.getElementById("pmDistrict").disabled = true;
		document.getElementById("pmSubDistrict").disabled = true;

	} else {

		document.getElementById("pmSubDistrict").disabled = false;
		document.getElementById("pmHouseNo").disabled = false;
		document.getElementById("pmStreet").disabled = false;
		document.getElementById("pmTown").disabled = false;
		document.getElementById("pmDistrict").disabled = false;
		document.getElementById("pmPinCode").disabled = false;
		document.getElementById("pmState").disabled = false;

	}
}

