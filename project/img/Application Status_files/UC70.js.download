function removeRow(id) {

	document.getElementById('dob').style.pointerEvents = 'none';
	// document.getElementById("covIssuDate").value="";
	var opt = confirm("Are you sure want to delete the Selected Rows");

	if (opt) {
		var objTable = document.getElementById(id);
		var counter = 0, intialtotalRows = 0;
		var intialtotalRows = objTable.rows.length;
		// alert("objTable.rows.length " + objTable.rows.length);

		if (objTable.rows.length > 1) {
			for (var i = 1; i < objTable.rows.length; i++) {
				var chk = objTable.rows[i].cells[0].childNodes[0];
				// alert("chk.checked " + chk.checked);
				if (chk.checked) {

					// alert("deleted recodrd i val " + i);
					objTable.deleteRow(i);
					i--;
					counter = counter + 1;
				}
			}

			if (counter > 0) {
				var rowcount = document.getElementById('covTable').rows.length;

				if (rowcount == 1) {
					document.getElementById('dlIssueDate').style.pointerEvents = 'visible';
					document.getElementById("dlIssueDate").disabled = false;
					document.getElementById("ntFrDate").value = "";
					document.getElementById("ntToDate").value = "";
					document.getElementById("trFrDate").value = "";
					document.getElementById("trToDate").value = "";
				}
				document.getElementById("addButton").disabled = false;
				// document.getElementById("editButton").disabled =
				// false;//murali
				document.getElementById("editButton").disabled = true;
				document.getElementById("removeButton").disabled = true;

				clearTxtControls();
			} else if (counter == 0)

			{

				// alert("Please select the row that you want to delete.");
				document.getElementById("addButton").disabled = false;
				// document.getElementById("editButton").disabled =
				// false;//murali
				document.getElementById("editButton").disabled = true;
				document.getElementById("removeButton").disabled = true;
			}
		} else {

			/** * Alert user if there are no rows being added ** */
			alert("There are no rows being added");
		}
	}

}

