function calculateHZTdate() {
	var js_hzFrDate = document.getElementById("hzFrDate");
	var js_trToDate = document.getElementById("hzToDate");

	var trfDate = js_hzFrDate.value.substring(0, 2);
	var hiphen5 = js_hzFrDate.value.substring(2, 3);
	var hiphen6 = js_hzFrDate.value.substring(5, 6);
	var trfMonth = js_hzFrDate.value.substring(3, 5);
	var trfYear = js_hzFrDate.value.substring(6, 10);

	var trFromDate = new Date(trfYear, trfMonth - 1, trfDate - 1);
	trFromDate.setFullYear(1 + trFromDate.getFullYear());
	var day = ('0' + trFromDate.getDate()).slice(-2);
	var month = ('0' + (trFromDate.getMonth() + 1)).slice(-2);
	var year = trFromDate.getFullYear();

	var validToDate = day + "-" + month + "-" + year;

	var js_trToDate = document.getElementById("trToDate");
	var trToDate = js_trToDate.value;
	// alert(trToDate);
	var js_hzFromDate = document.getElementById("hzFrDate").value;

	var hzvalToSplitDt = validToDate.split("-");
	var toDtSplitVal = trToDate.split("-");
	vaDt = Date.UTC(hzvalToSplitDt[2], hzvalToSplitDt[1] - 1,
			hzvalToSplitDt[0], 0, 0, 0, 0); // month is 0 to 11 not 1 to 12
	hzToDt = Date.UTC(toDtSplitVal[2], toDtSplitVal[1] - 1, toDtSplitVal[0], 0,
			0, 0, 0);

	if (hzToDt >= vaDt) {
		// alert("wrong 1");
		document.getElementById("hzToDate").value = validToDate;
	} else {
		// alert("wrong 2");
		document.getElementById("hzToDate").value = trToDate;
	}
	// document.getElementById("hzToDate").value = validToDate;

}

