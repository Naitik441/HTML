function removeAllOptions(selectbox) {
	var i;
	for (i = selectbox.options.length - 1; i >= 0; i--) {
		selectbox.remove(i);
	}
}

function removeItem(res) {
	var i;
	for (i = res.options.length - 1; i >= 0; i--) {
		if (res.options[i].selected)
			res.remove(i);
	}
}
