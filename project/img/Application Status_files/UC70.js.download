function displayVillageList() {
	// alert("Viollege List..........1111");
	var village = false;
	var town = false;
	// alert("get District Detais ! ");
	var prSubDistrict = $("#prSubDistrict").val();
	village = $('#presVillageOrTown1').is(':checked');
	town = $('#presVillageOrTown2').is(':checked');
	// alert("State = == = ? " + prState);
	if (village && !town) {
		// alert("village");
		$
				.ajax({
					type : 'POST',
					url : 'getVillagesbyDistrict.do?prSubDistrict='
							+ prSubDistrict,
					dataType : 'json',
					success : function(data) {
						var options = "<option value='-1' selected='selected'>Select Village</option>";
						// alert(" data= " + data.length);
						// alert(" value= " + key + " "+value);
						$.each(data, function(key, value) {

							options += "<option value='" + key + "'>"
									+ value.toUpperCase() + "</option>";
						});
						$("#prTown").html(options);
					}
				});
	} else {
		// alert("town");
		// $("#prTown").html("");
		$
				.ajax({
					type : 'POST',
					url : 'getTownsbyDistrict.do?prSubDistrict='
							+ prSubDistrict,
					dataType : 'json',
					success : function(data) {
						var options = "<option value='-1' selected='selected'>Select Town</option>";
						// alert(" data= " + data );
						if (data != null) {
							$.each(data, function(key, value) {

								options += "<option value='" + key + "'>"
										+ value.toUpperCase() + "</option>";
							});
						}
						// alert(options);
						$("#prTown").html(options);
					}
				});
	}

	// document.LLbacklogForm.action = "LLBacklog.do";
	// document.LLbacklogForm.submit();
}

