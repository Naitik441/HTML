/* <sample */
function setLocation(id, v) {
	// alert(id + " . . " + v);
	if (id == 'present') {
		if (v == 'S' && $("#prState").val() != '-1') {
			$("#prLocation").val($("#prState").val());
		}

		if (v == 'S' && $("#prState").val() == '-1') {

			$("#prLocation").val('');
		}

		if (v == 'D' && $("#prDistrict").val() != '-1') {
			$("#prLocation").val(
					$(
							"#prDistrict option[value='"
									+ $("#prDistrict").val() + "']").text()
							+ "," + $("#prState").val());
		}
		if (v == 'D' && $("#prDistrict").val() == '-1') {
			$("#prLocation").val($("#prState").val());
		}

		if (v == 'SD' && $("#prSubDistrict").val() != '-1') {
			$("#prLocation").val(
					$(
							"#prSubDistrict option[value='"
									+ $("#prSubDistrict").val() + "']").text()
							+ ","
							+ $(
									"#prDistrict option[value='"
											+ $("#prDistrict").val() + "']")
									.text() + "," + $("#prState").val());
		}

		if (v == 'SD' && $("#prSubDistrict").val() == '-1') {
			$("#prLocation").val(
					$(
							"#prDistrict option[value='"
									+ $("#prDistrict").val() + "']").text()
							+ "," + $("#prState").val());
		}
		if (v == 'TV' && $("#prTown").val() != '-1') {
			$("#prLocation").val(
					$("#prTown option[value='" + $("#prTown").val() + "']")
							.text()
							+ ","
							+ $(
									"#prDistrict option[value='"
											+ $("#prDistrict").val() + "']")
									.text() + "," + $("#prState").val());
		}
		if (v == 'TV' && $("#prTown").val() == '-1') {
			$("#prLocation").val(
					$(
							"#prSubDistrict option[value='"
									+ $("#prSubDistrict").val() + "']").text()
							+ ","
							+ $(
									"#prDistrict option[value='"
											+ $("#prDistrict").val() + "']")
									.text() + "," + $("#prState").val());
		}
	}
	if (id == 'permanent') {

		if (v == 'S' && $("#pmState").val() != '-1') {
			$("#pmLocation").val($("#pmState").val());
		}

		if (v == 'S' && $("#pmState").val() == '-1') {

			$("#pmLocation").val('');
		}

		if (v == 'D' && $("#pmDistrict").val() != '-1') {
			$("#pmLocation").val(
					$(
							"#pmDistrict option[value='"
									+ $("#pmDistrict").val() + "']").text()
							+ "," + $("#pmState").val());
		}
		if (v == 'D' && $("#pmDistrict").val() == '-1') {
			$("#pmLocation").val($("#pmState").val());
		}

		if (v == 'SD' && $("#pmSubDistrict").val() != '-1') {
			$("#pmLocation").val(
					$(
							"#pmSubDistrict option[value='"
									+ $("#pmSubDistrict").val() + "']").text()
							+ ","
							+ $(
									"#pmDistrict option[value='"
											+ $("#pmDistrict").val() + "']")
									.text() + "," + $("#pmState").val());
		}

		if (v == 'SD' && $("#pmSubDistrict").val() == '-1') {
			$("#pmLocation").val(
					$(
							"#pmDistrict option[value='"
									+ $("#pmDistrict").val() + "']").text()
							+ "," + $("#pmState").val());
		}
		if (v == 'TV' && $("#pmTown").val() != '-1') {
			$("#pmLocation").val(
					$("#pmTown option[value='" + $("#pmTown").val() + "']")
							.text()
							+ ","
							+ $(
									"#pmDistrict option[value='"
											+ $("#pmDistrict").val() + "']")
									.text() + "," + $("#pmState").val());
		}
		if (v == 'TV' && $("#pmTown").val() == '-1') {
			$("#pmLocation").val(
					$(
							"#pmSubDistrict option[value='"
									+ $("#pmSubDistrict").val() + "']").text()
							+ ","
							+ $(
									"#pmDistrict option[value='"
											+ $("#pmDistrict").val() + "']")
									.text() + "," + $("#pmState").val());
		}

	}
}

