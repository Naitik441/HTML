function getDistrictsListForPerm() {
	// alert('Inside Functionoooooo');
	var stateCode = $('#prState').val();
	// alert('State Code : '+stateCode);
	$.ajax({
		type : 'POST',
		url : 'getDistrictListForPerm.do?prState=' + stateCode,
		dataType : 'json',
		success : function(data) {
			var options = "<option value='-1'>Select</option>";
			$.each(data.distList,
					function(key, value) {
						options += "<option value='" + key + "'>" + value
								+ "</option>";
					});
			$("#prDistrict").html(options);
			$('#presSubDistrict_label').text(data.subDistLabel);
		}
	});
}

