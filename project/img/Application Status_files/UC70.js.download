function showHideIDPDef() {
	/*
	 * if($("input[name=idpdefence]:checked").val()==-1){
	 * $("#idpDetails").hide(); $("#defenceDetails").hide();
	 * $("#foriegnDetails").hide(); }
	 */

	if ($("input[name=idpdefence]:checked").val() == 1) {

		$("#idpDetails").show('slow');
		$("#defenceDetails").hide();
		$("#foriegnDetails").hide();
	} else if ($("input[name=idpdefence]:checked").val() == 2) {
		$("#foriegnDetails").show('slow');
		$("#idpDetails").hide();
		$("#defenceDetails").hide();
	} else if ($("input[name=idpdefence]:checked").val() == 3) {
		$("#defenceDetails").show('slow');
		$("#idpDetails").hide();
		$("#foriegnDetails").hide();
	} else {
		$("#idpDetails").hide();
		$("#defenceDetails").hide();
		$("#foriegnDetails").hide();
	}
}

function checkPinCodeInServices(addressType) {
	//alert("pincode--->");
	var pinCode = null;
	var url = null;

	if (addressType == 'permanent') {
		pinCode = $('#pmPinCode').val();
		url = 'checkPinCodeInBacklog.do?pmPinCode=' + pinCode;
	} else if (addressType == 'present') {
		pinCode = $('#prPinCode').val();
		url = 'checkPinCodeInBacklog.do?prPinCode=' + pinCode;
	}
	//alert("pincode--->"+pinCode);
	if (pinCode.length == 6) {
		$.ajax({

			type : 'POST',
			dataType : 'json',
			url : url,
			success : function(data) {
				//alert("pincode--1->"+pinCode);
				if (data.pmState == null || data.pmState == 'null') {
					//alert("pincode--2->"+pinCode);
					if (addressType == 'permanent') {
						alert('Enter Valid Pincode in Permanent Address');
						$('#pmPinCode').val('');
						$('#pmPinCode').focus();
					} else if (addressType == 'present') {
						alert('Enter Valid Pincode in Present Address');
						$('#prPinCode').val('');
						$('#prPinCode').focus();
					}
				}
			}

		});
	}
}
