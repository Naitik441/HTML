//Check valid pin or not in permanent addesssu
function isValidPinOrNot(addressType) {
	//alert("pincode");
	var pincode;
	var district;
	if(addressType == 'permanent'){
		pincode = $("#pmPinCode").val();
		district = $("#pmDistrict").val();
		state = $("#pmState").val();		
	}else if(addressType == 'present'){
		pincode = $("#prPinCode").val();
		district = $("#prDistrict").val();
		state1 = $("#prState").val();
	}
	//alert("addressType---"+addressType);
	//alert("state---"+state)
	$.ajax({
		type : 'POST',
		url : 'checkValidPinInPermAddressInBacklog.do?',
		data:{
			permPinCodeBacklog :pincode,
			permDistrictBacklog :district,
		},
		dataType : 'json',
		success : function(data) {
			if(addressType=='permanent'){
				if(state == '00'){
					if(!data.checkValidPin){
						alert("Enter Valid Pincode");
						$("#pmPinCode").val('');
						$("#pmPinCode").focus();
					}
				}
			}else if(addressType=='present'){
				if(state1 == 'MP'){
					if(!data.checkValidPin){
						alert("Entered present Pincode is not mapped with present District. So please change or contact RTO Office.");
						$("#prPinCode").val('');
						$("#prPinCode").focus();
					}
				}
			}
		}
	});
}


