		var adob = "01-06-" + (seryr - age.value).toString();

		var date = adob.substring(0, 2);
		var hiphen3 = adob.substring(2, 3);
		var hiphen4 = adob.substring(5, 6);
		var month = adob.substring(3, 5);
		var year = adob.substring(6, 10);

		if (year > (edYear - 16)) {
			alert("The Diff. between Issue Date and DOB must be greater than or equal to 16 Yrs");
