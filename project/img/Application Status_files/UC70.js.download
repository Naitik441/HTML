function calculateHilldate() {
	var js_ntFrDate = document.getElementById("hillFrDate");

	var ntfDate = js_ntFrDate.value.substring(0, 2);
	var hiphen5 = js_ntFrDate.value.substring(2, 3);
	var hiphen6 = js_ntFrDate.value.substring(5, 6);
	var ntfMonth = js_ntFrDate.value.substring(3, 5);
	var ntfYear = js_ntFrDate.value.substring(6, 10);
	
	
	if (js_ntFrDate.value != "") {
		if (hiphen5 !== "-" && hiphen6 !== "-") {
			alert("Enter Hill Validity From Date in dd-MM-yyyy Format \nEx:22-04-1991");
			document.backlogForm.hillFrDate.value = '';

			js_ntToDate.focus();
			return false;
		}
		if (js_ntFrDate.value.length != 10) {
			alert("Enter Hill Validity From Date in dd-MM-yyyy Format \nEx:22-04-1991");
			document.backlogForm.hillFrDate.value = '';

			js_ntFrDate.focus();
			return false;
		}
	}

	var ntFromDate = new Date(ntfYear, ntfMonth - 1, ntfDate - 1);
	ntFromDate.setFullYear(1 + ntFromDate.getFullYear());
	var day = ('0' + ntFromDate.getDate()).slice(-2);
	var month = ('0' + (ntFromDate.getMonth() + 1)).slice(-2);
	var year = ntFromDate.getFullYear();

	var validToDate = day + "-" + month + "-" + year;

	document.getElementById("hillToDate").value = validToDate;

}
