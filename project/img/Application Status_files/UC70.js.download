function checkRelation() {
	if ($('#relation').val() == 'H') {
		if ($('#gender1').is('checked'))
			$('#gender1').prop('checked', false);
		$('#gender1').prop('disabled', true);
	} else {
		$('#gender1').prop('disabled', false);
	}
}
function displayRtoList(val) {

	// alert("State--1");
	// alert(val);
	var state;
	if (val == 1) {
		state = $("#state").val();
	} else if (val == 2) {
		state = $("#state2").val();
	} else if (val == 3) {
		state = $("#have_state").val();
	} 
	else if (val == 4) {
		state = $("#trstate").val();
		//alert("St4--"+state);
	}else {
		state = $("#state1").val();
	}
	 //alert("val-----"+val);

	$
			.ajax({
				type : 'POST',
				url : 'getRTOsbyStateForDLBacklog.do?state=' + state +'&val='+val,
				dataType : 'json',
				success : function(data) {
					var options = "<option value='-1' selected='selected'>SELECT RTO NAME</option>";
					var options1 = "<option value='-1' selected='selected'>SELECT RTO CODE</option>";
					$.each(data, function(key, value) {
						// alert(" value= " + value + " ");
						options += "<option value='" + key + "'>" + value
								+ "</option>";
						options1 += "<option value='" + key.toUpperCase()
								+ "'>" + key.toUpperCase() + "</option>";
					});
					// alert(val);
					if (val == 1) {
						// alert("Rto Code 1 First Issued");
						$("#rtoCode").html(options1);
						$("#rtoCodeissvalue").html(options);
						$("#issuAuthList").html(options);
					} else if (val == 2) {
						// $("#rtoCode").html(options);
						$("#rtoCode1").html(options);
						$("#rtoCodevalue").html(options1);
					} else if (val == 3) {
						// $("#rtoCode").html(options);
						$("#have_rto_name").html(options);
						// $("#rtoCodevalue").html(options1);
					} 
					else if (val == 4) {
						//alert("in 4----1-----");
						// $("#trrtoCode1").html(options);
						$("#trrtoCode").html(options);
						// $("#rtoCodevalue").html(options1);
					}else {

						$("#issuAuthList").html(options);
					}

					// $("#issuAuthList").html(options);

				}
			});

	// document.LLbacklogForm.action = "LLBacklog.do";
	// document.LLbacklogForm.submit();
}

function displayRtoListForPublic() {

	var state;
	state = $("#state2").val();
	// alert(state+"------State");

	$
			.ajax({
				type : 'POST',
				url : 'getRTOsbyStateForDLBacklogPublic.do?state2=' + state,
				dataType : 'json',
				success : function(data) {

					var options = "<option value='-1' selected='selected'>SELECT RTO NAME</option>";
					var options1 = "<option value='-1' selected='selected'>SELECT RTO CODE</option>";

					$.each(data, function(key, value) {

						if (key == '1') {
							alert(value);
							$("#state2").val("-1");
						} else {
							options += "<option value='" + key.toUpperCase()
									+ "'>" + value.toUpperCase() + "</option>";
							options1 += "<option value='" + key.toUpperCase()
